{
  "name": "yapp-lang",
  "displayName": "Yapp Language Support",
  "description": "YA++ Language Support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "yapp",
      "aliases": ["YA++", "yapp"],
      "extensions": [".yapp",".tapp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "yapp",
      "scopeName": "source.yapp",
      "path": "./syntaxes/yapp.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wk1093/yapp-vscode.git"
  }
}
